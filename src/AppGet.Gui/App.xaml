<Application x:Class="AppGet.Gui.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:framework="clr-namespace:AppGet.Gui.Framework">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <framework:CaliburnBootstrapper x:Key="GuiBootstrapper" />
                </ResourceDictionary>
                <ResourceDictionary Source="/ModernUI;component/Assets/ModernUI.xaml" />
                <ResourceDictionary Source="/ModernUI;component/Assets/ModernUI.Light.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="AppGetAccent" Color="#FF065BB0"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
